

<section class="product spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-4">
                <div class="section-title">
                    <h4>Womens product</h4>
                </div>
            </div>
            <div class="col-lg-8 col-md-8">
                <ul class="filter__controls">
                    <li  data-filter="*">All</li>
                    <li class="active" data-filter=".women">Women</li>
                    <li data-filter=".men">Men</li>
                    <li data-filter=".kid">Kids</li>
                </ul>
            </div>
        </div>
        <div class="row property__gallery">
            <% womensProducts.forEach(element => { %>
            <div class="col-lg-3 col-md-4 col-sm-6 mix men">
                <div class="product__item">
                  
                    <div class="product__item__pic set-bg"  data-setbg="/product-images/<%= element.images[2] %>" onclick="location.href='/image/<%= element._id %>';">
                       
                        <div class="label new">New</div>
                        <ul class="product__hover">
                            <li>
                                <a href="img/product/product-177.jpg" class="image-popup"><span
                                        class="arrow_expand"></span></a>
                            </li>
                            <li>
                                <a href="#"><span class="icon_heart_alt"></span></a>
                            </li>
                            <li>
                                <a href="#"><span class="icon_bag_alt"></span></a>
                            </li>
                        </ul>
                    </div>
            
                    <div class="product__item__text">
                        <h6><a href="#"><%= element.productName %></a></h6>
                        <div class="rating">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                        <div class="product__price"><%= element.salePrice %></div>
                    </div>
                </div>
            </div>
            <% }) %>

        </div>













